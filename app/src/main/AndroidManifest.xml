<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.far.basesales">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:node="replace">
        <activity android:name=".MainReports"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".MaintenanceCompany"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AdminLicenseUsersControl"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AdminLicenseUserDevice"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AdminLicenseUsers"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AdminLicenseDevices"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AdminLicenseTokens"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainReceipt"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActualizationCenter"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainOrders"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MaintenanceClients"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AdminConfiguration"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".Login"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MaintenanceProductTypes"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MaintenanceUnitMeasure"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MaintenanceUsers"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MaintenanceUserTypes"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MaintenanceProductSubTypes"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MaintenanceProducts"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MaintenanceUsersControl"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainAssignation"
            android:screenOrientation="portrait" />
        <activity android:name="com.example.bluetoothlibrary.BluetoothScan" />
    </application>

</manifest>